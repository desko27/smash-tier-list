{
  "name": "smash-tier-list",
  "version": "1.6.3",
  "description": "ðŸŽ®ðŸŽ¯ Tier List application for Super Smash Bros series",
  "main": "src/index.js",
  "repository": "https://github.com/desko27/smash-tier-list.git",
  "author": "desko27 <desko27@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "env-cmd .env react-static start",
    "stage": "env-cmd .env cross-env ENV=staging react-static build --staging",
    "build": "env-cmd .env cross-env ENV=production react-static build",
    "stage:s3": "env-cmd .env cross-env DEPLOY=s3 ENV=staging react-static build --staging",
    "build:s3": "env-cmd .env cross-env DEPLOY=s3 ENV=production react-static build",
    "serve": "serve dist -p 3000",
    "eslint": "eslint . --ext=\"js,jsx\"",
    "mocha": "mocha -r babel-core/register --file scripts/testSetup.js \"src/**/*.test.js\"",
    "test": "nyc npm run mocha",
    "test:debug": "npm run mocha --inspect-brk",
    "coverage": "nyc --reporter=html --reporter=text npm run mocha",
    "ci:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "images:build": "yarn images:build:png && yarn images:build:webp",
    "images:build:png": "rimraf src/assets/img/chars/**/*.150.png && find src/assets/img/chars -name \\*.png -type f -exec convert {} -resize 150x150 {}.150.png \\;",
    "images:build:webp": "rimraf src/assets/img/chars/**/*.150.png.webp && find src/assets/img/chars -name \\*.150.png -type f -exec sh -c \"npx imagemin --plugin=webp {} > {}.webp\" \\;"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn eslint"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "history": "^4.7.2",
    "just-throttle": "^1.1.0",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-ga": "^2.5.3",
    "react-hot-loader": "^4.0.0-beta.21",
    "react-pose": "^4.0.6",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-static": "^5.9.7",
    "react-static-plugin-preact": "^6.3.0",
    "react-tippy": "^1.2.3",
    "redux": "^4.0.0",
    "reselect": "^3.0.1",
    "store": "^2.0.12",
    "styled-components": "^3.4.5",
    "styled-media-query": "^2.0.2",
    "use-force-update": "^1.0.2",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-transform-imports": "^1.5.1",
    "chai": "^4.1.2",
    "compression-webpack-plugin": "^1.1.12",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "env-cmd": "^8.0.2",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^3.0.1",
    "husky": "^1.0.0-rc.13",
    "image-webpack-loader": "^4.6.0",
    "imagemin-cli": "^4.0.0",
    "imagemin-webp": "^5.0.0",
    "jimp": "^0.5.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "responsive-loader": "^1.1.0",
    "rimraf": "^2.6.3",
    "serve": "^10.1.1",
    "svg-url-loader": "^2.3.2",
    "webpack-s3-plugin": "git+https://github.com/desko27/s3-plugin-webpack.git#master"
  }
}
