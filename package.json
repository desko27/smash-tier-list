    "styled-media-query": "^2.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "chai": "^4.1.2",
    "compression-webpack-plugin": "^1.1.12",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "env-cmd": "^8.0.2",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
